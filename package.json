{
  "name": "svelte-io",
  "version": "1.0.0",
  "description": "The IO monad for Svelte",
  "type": "module",
  "svelte": "index.js",
  "browser": "index.js",
  "module": "index.js",
  "main": "index.js",
  "scripts": {
    "test": "xo && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxfriends/svelte-io.git"
  },
  "keywords": [
    "svelte",
    "monad",
    "functional-programming",
    "fantasy-land"
  ],
  "author": "Cameron Eldridge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foxfriends/svelte-io/issues"
  },
  "homepage": "https://github.com/foxfriends/svelte-io#readme",
  "peerDependencies": {
    "svelte": "^3.39.0"
  },
  "dependencies": {
    "fantasy-land": "^5.0.0"
  },
  "devDependencies": {
    "ava": "^5.1.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "svelte": "^3.49.0",
    "xo": "^0.44.0"
  },
  "xo": {
    "space": true,
    "extensions": [
      "svelte",
      "js"
    ],
    "plugins": [
      "svelte3"
    ],
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "unicorn/filename-case": "off",
      "node/file-extension-in-import": "off",
      "padding-line-between-statements": "off"
    },
    "overrides": [
      {
        "files": [
          "**.svelte"
        ],
        "processor": "svelte3/svelte3",
        "rules": {
          "import/first": "off"
        }
      }
    ]
  }
}
